<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.glasgowteam12">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.HomeScreenActivity"/>
        <activity android:name=".Activities.Dashboard" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.InitialActivity" />
        <activity android:name=".Activities.UserCallActivity" />
        <activity android:name=".Activities.RespondentCallWait" />
        <activity android:name=".Activities.RespondentCallActivity"></activity>
    </application>

</manifest>